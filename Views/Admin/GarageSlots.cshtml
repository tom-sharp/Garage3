@model IEnumerable<Slot>
<partial name="_GaragesMenu" />

<table class="table">
	<thead>
		<tr>
			<th>@Html.DisplayNameFor(model => model.GarageId)</th>
			<th>@Html.DisplayNameFor(model => model.No)</th>
			<th>@Html.DisplayNameFor(model => model.InUse)</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var g in Model)
		{
			<tr>
				<td>@g.GarageId</td>
				<td>@g.No</td>
				<td>@g.InUse</td>
				<td>
				</td>
			</tr>
		}
	</tbody>
</table>

<div>
	<a asp-action="GaragesList">Back to List</a>
</div>
